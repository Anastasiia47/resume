<div class="card">
    <div class="card-body">
        {{#with nested_display}}
            <div class="display-1">{{title}}</div>
        <br>
        <div class="card">
        <div class="card-body">
        {{#with children_display}} 
             <div class="display-4">{{title}}</div>
             <p>{{text}}</p>
            {{#with button}}
                <div>
                    <button class="btn btn-outline-warning ">{{text}}</button>
                </div>
            {{/with}}
        {{/with}}
        <br>
        <h6>{{header_text}}</h6>
        {{#with button}}
            <button class="btn btn-info text-light">{{text}}</button>
        {{/with}}
        </div>
        </div> 
       {{/with}}
    </div>
</div>
<br>
<div class="card">
    <div class="card-body">
        {{#with paragraph}}
            <div class="text-uppercase">{{title}}</div>
            <br>
            <p class="text-lowercase">{{text}}</p>
           
            {{#with button}}
                <a class="btn btn-link" href={{url}}>{{text}}</a>
            {{/with}}
            <br>
            <div class="card">
             <div class="card-body">
                <ul>
                    {{#each list_name}}
                        <li>
                             <a class="btn btn-link" href={{url}}>{{text}}</a>
                        </li>
                    {{/each}}
                </ul>
            </div>
            </div>
            
        {{/with}}
    </div>
</div>






