<div class="card">
   <div class="card-body">
    <div class="card">
        <div class="card-body">
            {{#with heading}}
                {{#with main}}
                    <h1>{{title}}</h1>
                    <p>{{text}}</p>
                {{#with button_text}}
                    <div>
                        <button class="btn btn-primary">{{solid}}</button>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-primary ">{{outline}}</button>
                    </div>
                {{/with}}
                {{/with}}
        </div>
    </div>
        <br>
        <br>
        <div class="card">
            <div class="card-body">
                {{#with second}}
                    <h2>{{title}}</h2>
                    <p>{{text}}</p>
                {{#with button_text}}
                     <div>
                        <button class="btn btn-secondary">{{solid}}</button>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-secondary">{{outline}}</button>
                    </div>
                {{/with}}
                {{/with}}
            </div>
        </div> 
            <br>
            <br>
        <div class="card">
            <div class="card-body">
                {{#with subblock}}
                    <h3>{{title}}</h3>
                    <p>{{text}}</p>
                {{#with button_text}}
                <div>
                    <button class="btn btn-dark">{{solid}}</button>
                </div>
                <br>
                <div>
                    <button class="btn btn-outline-dark">{{outline}}</button>
                </div>
                {{/with}}
                {{/with}}
            </div>
        </div>  

      {{/with}}
        </div>
    </div>
    </div>
</div>

<br>
