<main>
    <section>
        {{#with navigation}}
            <div class="nav">
                {{#each links}}
                    <div class="nav-item col-2">
                        <a class="btn btn-link" href={{href}}>{{text}}</a>
                    </div>
               {{/each}}
               {{#with button}}
                    <div class="nav-item col-2">
                        <a class="btn btn-primary" href={{href}}>{{text}}</a>
                    </div>
               {{/with}}
                
            </div>
        {{/with}}
    </section>
    <section>
        {{#with header}}
        <h1>{{title}}</h1>
        <p>{{description}}</p> 
        {{#with button}}
            <a class="btn btn-primary" href={{link}}>{{text}}</a>
        {{/with}}
        {{/with}}
    </section>
    <hr>
    <section>
        {{#with city}}
            <h2>{{title}}</h2>
        
        <div  class="row gx-2 gy-4">
            {{#each cards}}
        <div class="col-5">
        <div class="card">
            {{#with picture}}
                 <img class="card-img top" src='https://picsum.photos/400/200' alt="New York">
            {{/with}}
        <div class="card-body">
            <h5 class="card-title">{{title}}</h5>
            <p>{{description}}</p>
            {{#with button}}
                <button class="btn btn-primary" href={{href}}>{{text}}</button>
            {{/with}}
        </div>
        </div>
        </div>
        {{/each}}
        </div>
     
        {{/with}}
    </section>

    <hr>

    <section>
        {{#with winner}}
            <h2>{{title}}</h2>
        {{#with table}}
            <table class="table">
                <thead>
                    <tr>
                        {{#each head}}
                            <th>{{this}}</th>
                        {{/each}}
                    </tr>
                </thead>
                <tbody>
                         {{#each user}}
                            <tr>
                            <th>{{place}}</th>
                            <td>{{name}}</td>
                            <td>{{surname}}</td>
                            <td>{{nickname}}</td>
                            </tr>
                        {{/each}}
                   
                </tbody>
            </table>
        {{/with}}
        {{/with}}
    </section>

    <hr>

    <section>
        {{#with gallery}}
            <h2>{{title}}</h2>
        <div class="row">
            {{#each cards}}
            <div class="col-4">
                <div class="card">
                    <div class="card-header">
                        {{name}}
                    </div>
                
                           <ul class="list-group list-group-flush">
                            {{#each items}}
                                 <li class="list-group-item">{{this}}</li>
                            {{/each}}
                            </ul>
                   
                 </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
    </section>

    <hr>

    <section>
        {{#with relaxion}}
             <div class="display-3">{{title}}</div>
            <div class="row gy-3 gx-2">
                {{#each cards}}
                <div class="col-5">
                   <div class="card text-bg-secondary">
                        <div class="card-header">{{header}}</div>
                        <div class="card-body">
                            <h5 class="card-title">{{title}}</h5>
                            <p>{{description}}</p>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        {{/with}}
    </section>

    <hr>

    <section>
        {{#with rest}}
            <div class="row">
                {{#each cards}}
                    <div class="col-6">
                        <div class="card  text-center">
                            <div class="card-header">{{header}}</div>
                            <div class="card-body">
                                <h5 class="card-title">{{title}}</h5>
                                <p>{{description}}</p>
                                <button class="btn btn-primary">{{button}}</button>
                                <br>
                                <br>
                            <div class="card-footer">{{footer}}</div>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        {{/with}}
    </section>

    <hr>

    <section>
        {{#with proposition}}
            <div class="display-2">{{title}}</div>
            <div class="row gy-5">
                {{#each card}}
                    <div class="col-5">
                        <div class="card text-bg-warning">
                            <div class="card-header">{{header}}</div>
                            <div class="card-body">
                                <h5 class="card-title">{{title}}</h5>
                                <p>{{description}}</p>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        {{/with}}
    </section>

    <hr>

 
    <section>
         {{#with footer}}
             <h5>{{title}}</h5>
             <div class="nav">
               
                <div class="nav-item col-2">
                    {{#each navigation}}
                         <a class="btn btn-link" href={{href}}>{{text}}</a>
                    {{/each}}
                </div>
                <div class="nav-item  col-3">
                    {{#each socialMedia }}
                         <a class="btn btn-outline-dark" link={{link}}>{{name}}</a>
                    {{/each}}
                </div>
                  <div class="nav-item  col-3">
                    {{#each partners}}
                         <a class="btn btn-outline-info" link={{link}}>{{name}}</a>
                    {{/each}}
                </div>


              
             </div>
         {{/with}}
    </section>

 




   
</main>








